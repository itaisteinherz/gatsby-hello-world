{
	"name": "gatsby-hello-world",
	"version": "1.0.0",
	"description": "Gatsby hello world blog",
	"license": "MIT",
	"repository": "itaisteinherz/gatsby-hello-world",
	"author": {
		"name": "Itai Steinherz",
		"email": "itaisteinherz@gmail.com",
		"url": "github.com/itaisteinherz"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"develop": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"test": "xo"
	},
	"keywords": [
		"gatsby",
		"hello world",
		"demo",
		"blog"
	],
	"dependencies": {
		"gatsby": "^2.7.1",
		"gatsby-source-filesystem": "^2.0.37",
		"gatsby-transformer-remark": "^2.3.12",
		"react": "^16.8.6",
		"react-dom": "^16.5.1"
	},
	"devDependencies": {
		"eslint-config-xo-react": "^0.19.0",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-react-hooks": "^1.6.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"extends": "xo-react",
		"envs": [
			"node",
			"browser"
		],
		"ignores": [
			".cache",
			"public"
		],
		"rules": {
			"quotes": [
				"error",
				"double"
			],
			"react/prop-types": "off",
			"unicorn/filename-case": "off"
		}
	}
}
